<schemalist><schema id="${GBD_NAME}" path="${GBD_PATH_TERMED}">
	<key name="keywidth" type="i">
		<range min="1" />
		<default>50</default>
	</key>
	<key name="keyheight" type="i">
		<range min="1" />
		<default>50</default>
	</key>
	<key name="maxheight" type="d">
		<range min="0" max="1" />
		<default>0.4</default>
	</key>
	<key name="xpadding" type="d">
		<range min="0" max="1" />
		<default>0.05</default>
	</key>
	<key name="ypadding" type="d">
		<range min="0" max="1" />
		<default>0.05</default>
	</key>
	<key name="reset" type="b">
		<default>true</default>
	</key>
	<key name="north" type="b">
		<default>false</default>
	</key>
	<key name="statusicon" type="s">
		<default>"${CMAKE_INSTALL_PREFIX}/share/gboard/gboard-icon.png"</default>
	</key>
	<key name="layout" type="s">
		<default>"file://${CMAKE_INSTALL_PREFIX}/share/gboard/layouts/de-latin1-short.gboard"</default>
	</key>
	<key name="fontsize" type="d">
		<range min="0" />
		<default>0.7</default>
	</key>
	<key name="relative" type="b">
		<default>true</default>
	</key>
	<key name="hardhide" type="b">
		<default>false</default>
	</key>
	<key name="multipointer" type="b">
		<default>false</default>
	</key>
	<key name="shaped" type="b">
		<default>true</default>
	</key>
	<key name="leftmenu" type="b">
		<default>false</default>
	</key>
</schema></schemalist>
